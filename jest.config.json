{
  "testEnvironment": "node",
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js"
  ],
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "transform": {},
  "testTimeout": 30000,
  "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
